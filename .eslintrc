{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
      	"amd": true,
        "jest": true,
      	"es6": true,
      	"node": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
    	"no-console": "warn",
        "no-unused-vars": "warn"
    },
    "settings": {
        "react": {
            "pragma": "React", 
            "version": "detect"
        }
    }
}